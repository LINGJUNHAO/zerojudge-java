package zerojudge;

import java.util.Scanner;

public class zerojudge {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        String secretstring;
        while (input.hasNext()) {
            secretstring = input.nextLine();
            for (int i = 0; i < secretstring.length(); i++) {
                System.out.print((char) (secretstring.charAt(i) - 7));
            }
            System.out.println();
        }
    }
}
